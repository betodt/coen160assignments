package diaztostado.assign1;

public class Rectangle {
	private int length;
	private int width;
	
	public Rectangle() {
		length = 0;
		width = 0;
	}
	
	public Rectangle (int newLength, int newWidth) {
		length = newLength;
		width = newWidth;
	}
	
	public int getLength() {
		return length;
	}
	
	public int getWidth() {
		return width;
	}
	
	public void setLength(int newLength) {
		length = newLength;
	}
	
	public void setWidth(int newWidth) {
		width = newWidth;
	}
	
	public int Area() {
		return length * width;
	}
}
