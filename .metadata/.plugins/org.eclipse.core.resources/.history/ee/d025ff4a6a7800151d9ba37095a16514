package diaztostado.assign1;

public class FramedPicture {
	private Frame frame;
	private Picture picture;

	public FramedPicture() {
		this.frame = new Frame();
		this.picture = new Picture();
	}
	
	public FramedPicture(int fLength, int fWidth, String material, int pLength, int pWidth, String title, String artist) {
		this.frame = new Frame(fLength, fWidth, material);
		this.picture = new Picture(pLength, pWidth, title, artist);
	}

}
